// Watermark image
const watermarkCanvas = createCanvas(600, 600);
const wmCtx = watermarkCanvas.getContext('2d');

wmCtx.fillStyle = 'rgba(37, 99, 235, 0.05)';
wmCtx.fillRect(0, 0, 600, 600);

// Infinity pattern
wmCtx.strokeStyle = 'rgba(124, 58, 237, 0.1)';
wmCtx.lineWidth = 3;

for (let x = 50; x < 600; x += 120) {
  for (let y = 50; y < 600; y += 120) {
    wmCtx.beginPath();
    wmCtx.arc(x, y, 20, 0.5 * Math.PI, 1.5 * Math.PI);
    wmCtx.arc(x + 40, y, 20, 1.5 * Math.PI, 0.5 * Math.PI);
    wmCtx.stroke();
  }
}